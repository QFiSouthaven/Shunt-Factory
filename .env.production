# Production Environment Configuration
# This file is loaded when VITE_APP_ENV=production

VITE_APP_ENV=production
VITE_APP_VERSION=2.0.0-professional

# Feature Flags - Production
VITE_ENABLE_TELEMETRY=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_TOOLS=false

# Logging - Errors only in production
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false

# Performance
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security - Strict
VITE_ENABLE_CSP=true
VITE_ENABLE_SANITIZATION=true

# Rate Limiting - Strict
VITE_RATE_LIMIT_SHUNT=50
VITE_RATE_LIMIT_WEAVER=10
VITE_RATE_LIMIT_FOUNDRY=20

# API Configuration
VITE_API_TIMEOUT=30000

# Backend API - Production URL
VITE_API_BASE_URL=https://api.aethershunt.com

# Error Tracking - Production Sentry DSN
VITE_SENTRY_DSN=

# Analytics - Production Google Analytics
VITE_GA_TRACKING_ID=

# Posthog - Production Analytics
VITE_POSTHOG_KEY=
VITE_POSTHOG_HOST=https://app.posthog.com
