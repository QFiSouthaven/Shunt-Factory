{
  "compilerOptions": {
    "target": "ES2022",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "module": "ESNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "types": [
      "node",
      "vite/client"
    ],
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "moduleDetection": "force",
    "allowJs": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "backend",
    "multi-agent-containers",
    "backup",
    "e2e",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/__tests__/**",
    "components/common/AsyncErrorBoundary.tsx",
    "components/common/FeatureErrorBoundary.tsx"
    "backend",
    "node_modules",
    "dist",
    "e2e",
    "backup",
    "multi-agent-containers"
  ]
}