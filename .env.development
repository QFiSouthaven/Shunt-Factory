# Development Environment Configuration
# This file is loaded automatically in development mode

VITE_APP_ENV=development
VITE_APP_VERSION=2.0.0-professional

# Feature Flags - Development
VITE_ENABLE_TELEMETRY=true
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_TOOLS=true

# Logging - Verbose in development
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOGS=true

# Performance
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security - Less strict in development
VITE_ENABLE_CSP=false
VITE_ENABLE_SANITIZATION=true

# Rate Limiting - More lenient in development
VITE_RATE_LIMIT_SHUNT=100
VITE_RATE_LIMIT_WEAVER=50
VITE_RATE_LIMIT_FOUNDRY=50

# API Configuration
VITE_API_TIMEOUT=60000

# Backend API (when available)
VITE_API_BASE_URL=http://localhost:8000
