# Frontend Environment Variables
# Copy this to .env.local for local development

# Backend API URL (Shunt Factory backend)
VITE_BACKEND_URL=http://localhost:8080

# API Key for backend authentication
VITE_API_KEY=dev-test-key

# Multi-Agent Orchestrator URL (Docker containers)
VITE_ORCHESTRATOR_URL=http://localhost:8090

# Feature Flags
VITE_USE_MULTI_AGENT=true  # Set to true to use Docker multi-agent system
VITE_USE_BACKEND=false     # Set to true to use secure backend API

# Note: The Gemini API key is NO LONGER needed in the frontend
# It is securely stored in the backend and Docker containers
