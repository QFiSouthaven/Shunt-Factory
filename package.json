{
  "name": "shunt-final-v",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "cross-env VITE_APP_ENV=development vite build",
    "build:staging": "cross-env VITE_APP_ENV=staging vite build",
    "build:prod": "cross-env VITE_APP_ENV=production vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "clean": "rimraf dist node_modules/.vite",
    "analyze": "vite-bundle-visualizer",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google/genai": "^1.28.0",
    "@xenova/transformers": "^2.17.2",
    "diff": "^8.0.2",
    "jszip": "^3.10.1",
    "pdfjs-dist": "^5.4.296",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reactflow": "^11.11.4",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^4.0.10",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^10.1.0",
    "happy-dom": "^20.0.10",
    "js-yaml": "^4.1.1",
    "jsdom": "^27.2.0",
    "rimraf": "^6.1.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^4.0.10"
  },
  "overrides": {
    "glob": ">=11.1.0"
  }
}
