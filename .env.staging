# Staging Environment Configuration
# This file is loaded when VITE_APP_ENV=staging

VITE_APP_ENV=staging
VITE_APP_VERSION=2.0.0-professional

# Feature Flags - Staging (production-like)
VITE_ENABLE_TELEMETRY=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_TOOLS=true

# Logging - Info level for staging
VITE_LOG_LEVEL=info
VITE_ENABLE_CONSOLE_LOGS=true

# Performance
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security - Production-like
VITE_ENABLE_CSP=true
VITE_ENABLE_SANITIZATION=true

# Rate Limiting - Production-like
VITE_RATE_LIMIT_SHUNT=50
VITE_RATE_LIMIT_WEAVER=10
VITE_RATE_LIMIT_FOUNDRY=20

# API Configuration
VITE_API_TIMEOUT=30000

# Backend API - Staging URL
VITE_API_BASE_URL=https://staging-api.aethershunt.com

# Error Tracking
# Add your Sentry DSN for staging
VITE_SENTRY_DSN=
